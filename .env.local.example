# Firebase Client Keys (publicly visible)
# Find these in your Firebase project settings under "Your apps"
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
NEXT_PUBLIC_APP_URL=http://localhost:9002

# Firebase Admin Key (secret)
# This is the Base64 encoded JSON of your service account key
# See CHECKLIST.md for instructions on how to generate this
FIREBASE_SERVICE_ACCOUNT_KEY_BASE64=

# Offerwall Partner Secrets
# This should be a strong, random string that you also provide to your offerwall partner
OFFERWALL_PARTNER_SECRET=

# Offerwall App IDs
NEXT_PUBLIC_CPX_APP_ID=
NEXT_PUBLIC_CPX_IFRAME_HASH=
NEXT_PUBLIC_TIMEWALL_APP_ID=e97ac5e4eb1ca52d
