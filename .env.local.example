# Firebase Client SDK Keys - Copy from your Firebase project settings
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Firebase Admin SDK Key - Must be Base64 encoded
# See CHECKLIST.md for instructions
FIREBASE_SERVICE_ACCOUNT_KEY_BASE64=

# Offerwall Partner Secret - A random string you create for securing the webhook
OFFERWALL_PARTNER_SECRET=

# Offerwall API Keys - From your partner dashboards
NEXT_PUBLIC_TIMEWALL_APP_ID=
NEXT_PUBLIC_CPX_APP_ID=
NEXT_PUBLIC_CPX_IFRAME_HASH=

# App URL - The public URL of your deployed application
NEXT_PUBLIC_APP_URL=http://localhost:9003
